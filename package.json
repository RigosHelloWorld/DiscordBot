    "test": "jest",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "20.10.4",
    "aws-cdk": "2.118.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.118.0",
    "constructs": "^10.0.0",
    "dot": "^1.1.3",
    "env": "^0.0.2",
    "source-map-support": "^0.5.21"
  }
}
