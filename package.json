{
  "name": "discord_bot",
  "version": "0.1.0",
  "bin": {
    "discord_bot": "bin/discord_bot.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf ./build",
    "setup": "npm install &&  npm run build",
    "format": "prettier --write .",
